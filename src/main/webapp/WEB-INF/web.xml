<web-app xmlns="https://jakarta.ee/xml/ns/jakartaee" version="5.0">
  <display-name>NGCclone</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  
  <servlet>
    <servlet-name>signin</servlet-name>
    <servlet-class>com.app.signupservlet.signupservlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>signin</servlet-name>
    <url-pattern>/signup</url-pattern>
  </servlet-mapping>
 
  <servlet>
  <servlet-name>login</servlet-name>
    <servlet-class>com.app.signupservlet.signupservlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/loginpage</url-pattern>
  </servlet-mapping>
  
  
</web-app>
